{
  "version": 1,
  "paths": {
    "/loans": {
      "name": "/loans",
      "lambdaFunction": "loanHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "read",
          "update"
        ]
      }
    },
    "/ledger": {
      "name": "/ledger",
      "lambdaFunction": "loanHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ]
      }
    },
    "/ledger/{id}": {
      "name": "/ledger/{id}",
      "lambdaFunction": "loanHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ]
      }
    }
  }
}